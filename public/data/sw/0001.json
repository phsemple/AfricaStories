{
    "id": "0001",
    "language": "sw",
    "storyPath": "/data/sw/0001.json",
    "title": "Mwanamume mrefu",
    "illustrator": " Catherine Groenewald",
    "writtenby": " Cornelius Gulere",
    "readby": " Lauwo George",
    "translator": " Ursula Nafula",
    "level": 2,
    "pages": [
        {
            "text": "Mwanamume mrefu",
            "pagenum": 0,
            "imageSrc": "/images/0001/0001_00.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_00.mp3"
        },
        {
            "text": "Jembe lake lilikuwa fupi mno.",
            "pagenum": 1,
            "imageSrc": "/images/0001/0001_01.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_01.mp3"
        },
        {
            "text": "Mlango wake ulikuwa chini mno.",
            "pagenum": 2,
            "imageSrc": "/images/0001/0001_02.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_02.mp3"
        },
        {
            "text": "Kitanda chake kilikuwa kifupi mno.",
            "pagenum": 3,
            "imageSrc": "/images/0001/0001_03.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_03.mp3"
        },
        {
            "text": "Baisikeli yake ilikuwa fupi sana.",
            "pagenum": 4,
            "imageSrc": "/images/0001/0001_04.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_04.mp3"
        },
        {
            "text": "Mtu huyu alikuwa mrefu zaidi!",
            "pagenum": 5,
            "imageSrc": "/images/0001/0001_05.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_05.mp3"
        },
        {
            "text": "Alitengeneza mpini wa jembe mrefu sana.",
            "pagenum": 6,
            "imageSrc": "/images/0001/0001_06.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_06.mp3"
        },
        {
            "text": "Alitengeneza fremu ndefu za milango.",
            "pagenum": 7,
            "imageSrc": "/images/0001/0001_07.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_07.mp3"
        },
        {
            "text": "Alitengeneza kitanda kirefu sana.",
            "pagenum": 8,
            "imageSrc": "/images/0001/0001_08.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_08.mp3"
        },
        {
            "text": "Akanunua baisikeli iliyokuwa ndefu mno.",
            "pagenum": 9,
            "imageSrc": "/images/0001/0001_09.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_09.mp3"
        },
        {
            "text": "Aliketi juu ya kiti kilichokuwa juu sana. Alikula kwa kutumia uma iliyokuwa ndefu mno.",
            "pagenum": 10,
            "imageSrc": "/images/0001/0001_10.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_10.mp3"
        },
        {
            "text": "Aliacha nyumba yake na kuishi katika msitu mkubwa. Aliishi kwa miaka mingi.",
            "pagenum": 11,
            "imageSrc": "/images/0001/0001_11.jpg",
            "audioSrc": "/audio/sw/0001/0001_sw_11.mp3"
        }
    ]
}